{
    "dev": {
        "options": {
            "cssDir": "src/css",
            "imagesDir": "src/images",
            "environment": "development",
            "specify": ["sass/screen.scss"]
        }
    },
    "prod": {
        "options": {
            "sassDir": "sass",
            "cssDir": "src/css",
            "imagesDir": "src/images",
            "environment": "production",
            "specify": ["sass/screen.scss"],
            "outputStyle": "compressed"
        }
    }
}
